{
  "name": "contrastvisor",
  "version": "0.3.0",
  "description": "Spot details through your camera using principal components analysis and decorrelation stretch",
  "main": "index.js",
  "scripts": {
    "build": "browserify contrastvisor/index.js -o contrastvisor/lib/bundle.js",
    "watch": "watchify contrastvisor/index.js -o contrastvisor/lib/bundle.js -v",
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish": "cp contrastvisor/index.html contrastvisor/manifest.webmanifest contrastvisor/sw.js docs/ && cp contrastvisor/lib/bundle.js docs/lib/ && cp -r contrastvisor/imgs docs/"
  },
  "author": "Cedric Shock",
  "license": "MIT",
  "dependencies": {
    "gl-matrix": "^3.3.0",
    "hammerjs": "^2.0.8",
    "ml-matrix": "^6.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "twgl.js": "^4.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.3",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "html-webpack-plugin": "^5.5.0",
    "watchify": "^4.0.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ],
          "plugins": [
            "@babel/plugin-syntax-jsx",
            "@babel/plugin-transform-react-jsx"
          ]
        }
      ]
    ]
  }
}
